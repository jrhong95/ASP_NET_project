

<form>
    <label>Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount </label>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>데이터 출처</th>
                <th>분실날짜</th>
                <th>분류</th>
                <th>물건</th>
                <th>위치</th>
                <th>설명</th>
            </tr>
        </thead>
        <tbody>
            x
            @foreach (var found in Model)
            {
                <tr>
                    <td>@found.Found_data</td>
                    <td>@found.Found_DateTime</td>
                    <td>@found.Found_BigCate</td>
                    <td>
                        <a asp-controller="Found" asp-action="Detail" asp-route-Found_id ="@found.Found_id"> @found.Found_Name</a>
                    </td>
                    <td>@found.Found_GetPosition</td>
                    <td>@found.Found_Description</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="panel text-center">
        @for (var p = 1; p <= Model.PageCount; p++)
        {
            <a href="@Url.Action("Search", new { page = p })" class="btn btn-outline-primary">@p</a>
        }
    </div>
</form>