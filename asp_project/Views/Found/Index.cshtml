@{
}

<label>Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount </label>
<input type="text" value="분실물" />
<input type="text" value="분실위치(행정구)" />
<input type="datetime" value="분실 날짜" />
<button>검색</button>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>데이터 출처</th>
            <th>분실날짜</th>
            <th>분류</th>
            <th>물건</th>
            <th>위치</th>
            <th>설명</th>
        </tr>
    </thead>
    <tbody>
        x
        @foreach (var found in Model)
        {
            <tr>
                <td>@found.Found_data</td>
                <td>@found.Found_DateTime</td>
                <td>@found.Found_BigCate</td>
                <td>@found.Found_Name</td>
                <td>@found.Found_GetPosition</td>
                <td>@found.Found_Description</td>
            </tr>
        }
    </tbody>
</table>
<div class="panel text-center">
    @for (var p = 1; p <= Model.PageCount; p++)
    {
        <a href="@Url.Action("Index", new { page = p })" class="btn btn-outline-primary">@p</a>
    }
</div>
